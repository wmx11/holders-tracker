// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Blocks {
  id            Int  @id @default(autoincrement())
  firstBlock    Int
  previousBlock Int?
  lastBlock     Int
}

model Transfers {
  id      Int    @id @default(autoincrement())
  address String
  block   Int
}

model Holders {
  id      Int    @id @default(autoincrement())
  address String
  value   Float
}
