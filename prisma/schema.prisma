generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Blocks {
  id            Int      @id @default(autoincrement())
  firstBlock    Int
  previousBlock Int?
  lastBlock     Int
  updated_at    DateTime @default(now())
}

model Transfers {
  id         Int      @id @default(autoincrement())
  address    String
  block      Int
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}

model Holders {
  id         Int      @id @default(autoincrement())
  address    String
  value      Float
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}
